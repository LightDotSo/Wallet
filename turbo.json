{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"]
    },
    "build:dev": {
      "dependsOn": ["^build"]
    },
    "@lightwallet/background#build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"],
      "inputs": ["**"],
      "outputs": ["../../Application/Light Safari Extension/Resources/background.js"]
    },
    "@lightwallet/background#build:dev": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"],
      "inputs": ["**"],
      "outputs": ["../../Application/Light Safari Extension/Resources/background.js"]
    },
    "@lightwallet/content#build": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "env": ["CI", "JITSU_JS_KEY"],
      "outputs": [
        "public",
        "../../Application/Light Safari Extension/Resources/content.js"
      ]
    },
    "@lightwallet/content#build:dev": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "env": ["CI", "JITSU_JS_KEY"],
      "outputs": [
        "public",
        "../../Application/Light Safari Extension/Resources/content.js"
      ]
    },
    "@lightwallet/ethereum#build": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "outputs": [
        "../../Application/Light Safari Extension/Resources/ethereum.js"
      ]
    },
    "@lightwallet/ethereum#build:dev": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "outputs": [
        "../../Application/Light Safari Extension/Resources/ethereum.js"
      ]
    },
    "@lightwallet/inpage#build": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "outputs": [
        "../../Application/Light Safari Extension/Resources/inpage.js"
      ]
    },
    "@lightwallet/inpage#build:dev": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "outputs": [
        "../../Application/Light Safari Extension/Resources/inpage.js"
      ]
    },
    "@lightwallet/popup#build": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "env": ["CI", "JITSU_JS_KEY", "VERCEL"],
      "outputs": [
        ".next",
        "../../Application/Light Safari Extension/Resources/popup/**",
        "../../Application/Light Safari Extension/Resources/popup.html"
      ]
    },
    "@lightwallet/popup#build:dev": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "env": ["CI", "JITSU_JS_KEY", "VERCEL"],
      "outputs": [
        ".next",
        "../../Application/Light Safari Extension/Resources/popup/**",
        "../../Application/Light Safari Extension/Resources/popup.html"
      ]
    },
    "@lightwallet/wallet#build": {
      "dependsOn": ["^build"],
      "inputs": ["**"],
      "outputs": [
        ".next"
      ]
    },
    "storybook": {
      "dependsOn": ["^storybook"]
    },
    "//#lint": {
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "Extension/**/src/**/*.ts",
        "Extension/**/src/**/*.tsx"
      ]
    },
    "//#fastlane:submit": {
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "Application/**",
        "Components/**",
        "Extension/background/**",
        "Extension/content/**",
        "Extension/ethereum/**",
        "Extension/popup/**",
        "fastlane/**",
        "packgae.json"
      ]
    },
    "//#fastlane:test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "Application/**",
        "Components/**",
        "fastlane/**"
      ]
    }
  }
}